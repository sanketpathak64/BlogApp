<%- include ('partials/header'); -%>
<div class="container main"> <h1>Index page</h1>



    <%blogs.forEach(function(blog){%>
        <div>
            <h2><%=blog.title%></h2>
            <img src="<%=blog.image%>">
            <span><%= blog.created%></span>
            <p><%=blog.body.substring(0,100)%>....</p>
            <a href="/blogs/<%=blog._id%>">Read More</a>
        </div>
    <%})%>
</div>
<%- include ('partials/footer'); -%>